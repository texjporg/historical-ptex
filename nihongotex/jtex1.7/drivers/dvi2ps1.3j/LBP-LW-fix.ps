% Procedure to make a new font which has new metrics.
/Set-New-Metrics % Fontname New-Fontname Metrics-array Set-Font-Metrics
{/Set-Metrics exch def
 /NewCompFontName exch def
 findfont /OrigCompFont exch def

 % Create new composite font.
 OrigCompFont dup length dict /NewCompFont exch def
 {1 index /FID ne
  {NewCompFont 3 1 roll put}
  {pop pop}
  ifelse
 } forall
 OrigCompFont /FDepVector get dup length array copy
 /NewFDepVector exch def
 NewCompFont /FDepVector NewFDepVector put
 NewCompFontName NewCompFont definefont pop

 Set-Metrics	% [code NewBaseFontName Metrics-Proc]
 {dup 0 get /Code exch def
  dup 1 get /NewBaseFontName exch def
  2 get /Metrics-Proc exch def

  % Extract base font and its index to FDepVector.
  /Index OrigCompFont /Encoding get Code get def
  /OrigBaseFont NewFDepVector Index get def

  % Create new base font.
  OrigBaseFont dup length 1 add dict /NewBaseFont exch def
  {1 index /FID ne
   {NewBaseFont 3 1 roll put}
   {pop pop}
   ifelse
  } forall

  Metrics-Proc length dict begin
  Metrics-Proc {exec def} forall
  NewBaseFont /Metrics currentdict put
  end

  NewFDepVector Index NewBaseFontName NewBaseFont definefont put
 } forall
}
bind def


