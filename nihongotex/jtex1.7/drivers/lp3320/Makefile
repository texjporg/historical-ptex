all:: $(PROGRAMS)

dvi2lp3320: $(COMMONOBJS) lp3320.o
	$(RM) $@
	$(CC) -o $@ $(COMMONOBJS) lp3320.o $(LDOPTIONS)  $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) dvi2lp3320

