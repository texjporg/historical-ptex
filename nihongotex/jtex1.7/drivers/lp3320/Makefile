install:: $(PROGRAMS)
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(PROGRAMS); do \
	(set -x; $(INSTALL) -c $(INSTALLFLAGS) $$i  $(USRLIBDIR)); \
	done

depend::
	$(DEPEND) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM) $(PROGRAMS)

